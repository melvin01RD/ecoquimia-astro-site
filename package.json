{
  "name": "ecoquimia-astro-site",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "format": "prettier --write .",
    "check:astro": "astro check",
    "lint:html": "astro build && html-validate \"dist/**/*.html\"",
    "check": "npm run check:astro && npm run build && npm run lint:html",
    "deploy": "vercel --prod",
    "deploy:prebuilt": "vercel deploy --prod --prebuilt",
    "predeploy": "npm run check",
    "predeploy:prebuilt": "npm run check",
    "clean": "rimraf dist .astro"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "nodemailer": "^7.0.6",
    "resend": "^6.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@astrojs/sitemap": "^3.6.0",
    "@astrojs/tailwind": "^5.1.5",
    "@astrojs/vercel": "^8.2.8",
    "@lucide/astro": "latest",
    "@types/node": "^24.5.2",
    "@types/nodemailer": "^7.0.1",
    "astro": "^5.14.1",
    "autoprefixer": "^10.4.19",
    "html-validate": "^10.0.0",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.9.2"
  },
  "description": "Landing de Ecoquimia con Astro + Tailwind"
}
